																				<xs:annotation>
																					<xs:documentation>ИНформация о подписании</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="signrequired" type="xs:boolean">
																				<xs:annotation>
																					<xs:documentation>Требование о подписании файла</xs:documentation>
																				</xs:annotation>
