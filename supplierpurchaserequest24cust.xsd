																					<xs:sequence>
																						<xs:element name="doc">
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element name="file">
																										<xs:complexType>
																											<xs:sequence>
																												<xs:element name="fileid" type="xs:string"/>
																												<xs:element name="filename" type="xs:string"/>
